            </auxheader>  
        </auxhead>
        <auxhead>
            <auxheader align="center" colspan="2">                             
                <!--<image content="@bind(vm.foto)" align="center" width="300px"  height="200px"/>-->
            </auxheader>  
        </auxhead>
                    
        <listhead> 
            <listheader width="60%"  label="DescripciÃ³n" /> 
